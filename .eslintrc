{
    "rules": {
        "indent": [0, 2],
        "no-mixed-spaces-and-tabs": [0],
        "quotes": [0,"double"],
        "linebreak-style": [2, "unix"],
        "semi": [2,"always"],
        "no-console": [0]
    },
    "env": {
        "es6": true,
        "browser": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "ecmaFeatures": {
        "jsx": true,
        "experimentalObjectRestSpread": true,
        "modules": true
    },
    "plugins": [
        "react"
    ],

    "globals": {
        "React": false,
        "__DEV__": false,
        "$": false,
        "Backbone": false,
        "_": false
    }
}